{
  "name": "quad-browser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-windows": "electron-packager . --overwrite --icon=assets/icons/win/icon.ico --platform=win32 --arch=x64 --prune=true --out=dist",
    "package-mac": "electron-packager . --overwrite --icon=assets/icons/mac/icon.icns --platform=darwin --arch=x64 --prune=true --out=dist",
    "package-mac-dmg": "npm run package-mac & electron-installer-dmg dist/quad-browser-darwin-x64 QuadBrowser icon=assets/icons/mac/icon.icns"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^6.0.1",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.0.4"
  }
}
