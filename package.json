{
  "name": "QuadScreens",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-windows": "electron-packager . QuadScreens --overwrite --icon=assets/icons/win/icon.ico --platform=win32 --arch=x64 --prune=true --out=dist",
    "package-mac": "electron-packager . QuadScreens --overwrite --icon=./assets/icons/mac/icon.icns --platform=darwin --arch=x64 --prune=true --out=dist",
    "package-mac-dmg": "electron-installer-dmg ./dist/QuadScreens-darwin-x64/QuadScreens.app QuadScreens --out=dist/ --icon=assets/icons/mac/icon.icns --icon-size=80 --overwrite --debug"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^6.0.1",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.0.4"
  },
  "dependencies": {
    "electron-context-menu": "^0.14.0",
    "electron-prompt": "^1.3.1",
    "iohook": "^0.6.1"
  },
  "iohook": {
    "targets": [
      "node-64",
      "electron-73"
    ],
    "platforms": [
      "win32",
      "darwin"
    ],
    "arches": [
      "x64"
    ]
  }
}
